{
  "headers": [
    {
      "source": "/(.+)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, immutable"
        }
      ]
    },
    {
      "source": "/(index.html)?",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "no-store, no-cache"
        }
      ]
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}